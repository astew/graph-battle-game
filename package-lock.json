{
  "name": "graph-battle-game",
  "version": "0.1.0",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "graph-battle-game",
      "version": "0.1.0",
      "workspaces": [
        "packages/*",
        "apps/*"
      ]
    },
    "apps/ui": {
      "name": "@graph-battle/ui",
      "version": "0.1.0"
    },
    "node_modules/@graph-battle/bots": {
      "resolved": "packages/bots",
      "link": true
    },
    "node_modules/@graph-battle/core": {
      "resolved": "packages/core",
      "link": true
    },
    "node_modules/@graph-battle/simulator": {
      "resolved": "packages/simulator",
      "link": true
    },
    "node_modules/@graph-battle/ui": {
      "resolved": "apps/ui",
      "link": true
    },
    "packages/bots": {
      "name": "@graph-battle/bots",
      "version": "0.1.0",
      "license": "MIT",
      "dependencies": {
        "@graph-battle/core": "0.1.0"
      }
    },
    "packages/core": {
      "name": "@graph-battle/core",
      "version": "0.1.0",
      "license": "MIT"
    },
    "packages/simulator": {
      "name": "@graph-battle/simulator",
      "version": "0.1.0",
      "license": "MIT",
      "dependencies": {
        "@graph-battle/bots": "0.1.0",
        "@graph-battle/core": "0.1.0"
      },
      "bin": {
        "graph-battle-sim": "src/index.js"
      }
    }
  }
}
